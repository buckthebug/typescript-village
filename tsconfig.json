{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "output",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "strictNullChecks": false
  },
  "exclude": [
    "node_modules",
    "output"
  ]
}